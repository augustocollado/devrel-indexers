{
    "outDir": "src/evm-types",
    "specVersions": "evm-metadata.jsonl",
    "pallets": {
        "System": {
            "events": [
                "ExtrinsicSuccess",
                "ExtrinsicFailed",
                "NewAccount"
            ]
        },
        "Ethereum": {
            "calls": [
                "transact"
            ],
            "events": [
                "Executed"
            ]
        },
        "Balances": {
            "events": [
                "Transfer"
            ],
            "calls": [
                "transfer_all",
                "transfer_allow_death",
                "transfer_keep_alive"
            ]
        },
        "Utility": {
            "calls": [
                "batch",
                "batch_all"
            ]
        }
    }
}